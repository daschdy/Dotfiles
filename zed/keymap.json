[
  {
    "context": "Workspace",
    "bindings": {
      "space n": "workspace::ToggleLeftDock",
      "space t": "workspace::NewTerminal"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "space f f": [
        "workspace::SendKeystrokes",
        "cmd-p"
      ],
      "shift-k": [
        "workspace::SendKeystrokes",
        "cmd-shift-k"
      ],
      "alt-h": [
        "workspace::SendKeystrokes",
        "alt-cmd-left"
      ],
      "ctrl-h": "pane::ActivatePrevItem",
      "ctrl-l": "pane::ActivateNextItem"
    }
  }
]
